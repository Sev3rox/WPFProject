<Window x:Class="ProjektWPF.Druzyny.DeleteDruzyna"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjektWPF.Druzyny"
        mc:Ignorable="d"
        Title="DeleteDruzyna" Height="130" Width="400">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
            <RowDefinition/>
        </Grid.RowDefinitions>

        <Label Margin="5" FontSize="18" >Czy na pewno chcesz usunąć tą drużynę?</Label>
        <StackPanel HorizontalAlignment="Center" Grid.Row="2" Orientation="Horizontal">
            <Button FontSize="18" Click="Del"  Padding="10,3" Margin="5,5,80,5">Usuń</Button>
            <Button FontSize="18" Click="Cancel" Margin="5" Padding="10,3" >Anuluj</Button>

        </StackPanel>
    </Grid>
</Window>

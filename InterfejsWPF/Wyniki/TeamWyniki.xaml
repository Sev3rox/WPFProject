<Window x:Class="ProjektWPF.Wyniki.TeamWyniki"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjektWPF.Wyniki"
        mc:Ignorable="d"
        Title="TeamWyniki" Height="480" Width="400">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
            <RowDefinition />
            <RowDefinition Height="auto"/>
        </Grid.RowDefinitions>
        <Label Content="Infinity Białystok"></Label>
        <Label Content="Regional Triangles" Grid.Column="1"></Label>
        <ListBox Grid.Row="1" Margin="5"  DisplayMemberPath="NameAndPrice">
            <ListBoxItem Content="Jakub Błaszczykowski"></ListBoxItem>
        </ListBox>
                <ListBox Grid.Row="1" Grid.Column="1" Margin="5"  DisplayMemberPath="NameAndPrice">
            <ListBoxItem Content="Kamil Grosicki"></ListBoxItem>
        </ListBox>
        <Button Margin="10" Padding="10,6" FontSize="18"  HorizontalAlignment="Center" Grid.Column="0" Grid.ColumnSpan="2" Grid.Row="10">Zamknij</Button>
    </Grid>
</Window>
